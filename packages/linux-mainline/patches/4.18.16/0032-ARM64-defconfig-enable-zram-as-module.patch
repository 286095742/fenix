+CONFIG_ZRAM_WRITEBACK=y
 CONFIG_BLK_DEV_LOOP=y
 CONFIG_BLK_DEV_NBD=m
 CONFIG_VIRTIO_BLK=y
@@ -454,7 +457,6 @@ CONFIG_SND_BCM2835_SOC_I2S=m
 CONFIG_SND_SOC_MESON=m
 CONFIG_SND_SOC_MESON_I2S=m
 CONFIG_SND_SOC_MESON_SPDIF=m
